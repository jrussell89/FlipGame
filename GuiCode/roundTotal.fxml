<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FlipGame.GuiCode.RoundTotal">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" onMouseMoved="#init" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-color: green;" VBox.vgrow="ALWAYS">
      <children>
        <Label fx:id="header" alignment="CENTER" layoutX="258.0" layoutY="36.0" style="&#10;" text="End of Round #" textAlignment="CENTER" textFill="WHITE" underline="true" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Button layoutX="541.0" layoutY="322.0" mnemonicParsing="false" onAction="#startNext" onMouseMoved="#init" text="Next" />
            <Label fx:id="reportText" alignment="CENTER" layoutX="39.0" layoutY="74.0" style="&#10;" text="Round Report" textAlignment="CENTER" textFill="WHITE" underline="true" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="cashText" alignment="CENTER" layoutX="39.0" layoutY="101.0" style="&#10;" text="Cash - $# " textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="bankText" alignment="CENTER" layoutX="39.0" layoutY="128.0" style="&#10;" text="Bank Balance - $#" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="totalText" alignment="CENTER" layoutX="39.0" layoutY="155.0" style="&#10;" text="Total Money - $#" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="15.0" layoutY="353.0" style="&#10;" text="Move mouse to update!" textAlignment="CENTER" textFill="#9f9f9f" underline="true" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="pointsText" alignment="CENTER" layoutX="40.0" layoutY="181.0" style="&#10;" text="Question Points - #" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="cardText" alignment="CENTER" layoutX="40.0" layoutY="208.0" style="&#10;" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
   <effect>
      <ColorAdjust />
   </effect>
</VBox>
